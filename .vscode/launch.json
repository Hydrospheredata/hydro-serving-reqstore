    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch (GDBSERVER)",
			"type": "cppdbg",
			"request": "launch",
			"miDebuggerServerAddress": "localhost:2000",
			"additionalSOLibSearchPath": "${workspaceRoot}/src",
			"preLaunchTask": "start-gdbserver",
			"miDebuggerPath": "/usr/local/bin/gdb",
			"program": "${workspaceRoot}/src/reqstore",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"sourceFileMap":{
				"/home/develop/project": "${workspaceRoot}/src"
			},
			"environment": []
        }
    ]
}